<?xml version="1.0" encoding="utf-8"?>
<!-- This is the rating bar drawable that is used to a show a filled star. -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true" android:state_pressed="true"
          android:drawable="@drawable/ic_options_item_more"/>

    <item android:state_checked="true" android:state_focused="true"
          android:drawable="@drawable/ic_options_item_more"/>

    <item android:state_enabled="false" android:drawable="@drawable/icon_setting_arrow_disable" />
    <item android:drawable="@drawable/ic_options_item_more"/>

</selector>